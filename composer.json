{
    "name": "axelpal/sellerstore",
    "type": "project",
    "license": "Proprietary",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "ext-simplexml": "*",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "eusonlito/laravel-meta": "^3.1",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "iamcal/lib_autolink": "^1.7",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.7",
        "laravel/tinker": "^2.0",
        "paquettg/php-html-parser": "^2.0",
        "sentry/sentry-laravel": "^2",
        "spatie/laravel-responsecache": "^6.6",
        "spiral/roadrunner": "^1.8",
        "spiral/roadrunner-laravel": "^3.4",
        "stevegrunwell/time-constants": "^1.1",
        "tcg/voyager": "^1.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.4",
        "brainmaestro/composer-git-hooks": "^2.8",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "krlove/eloquent-model-generator": "^1.3",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.7.12",
        "phpstan/phpstan": "^0.12.93",
        "phpunit/phpunit": "^9.0",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "hooks": {
            "pre-commit": [
                "make hook-pre-commit"
            ],
            "pre-push": [
                "make hook-pre-push"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "stubs": [
            "@php artisan ide-helper:generate"
        ],
        "cghooks": "vendor/bin/cghooks",
        "post-install-cmd": "cghooks update --git-dir=../.git",
        "post-update-cmd": "cghooks update --git-dir=../.git"
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
